{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "tsc: build - tsconfig.json",
      "type": "shell",
      "command": "${workspaceFolder}/server/node_modules/.bin/tsc",
      "args": ["-p", "${workspaceFolder}/server/tsconfig.json"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": ["$tsc"],
      "detail": "Generated task by TypeScript extension.",
      "cwd": "${workspaceFolder}/server"
    }
  ]
}
